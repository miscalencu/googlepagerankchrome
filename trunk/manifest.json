{
	"name": "Lite Page Rank",
	"description" : "Gets Google's Page Rank and displays it in the address bar. Multiple icon sets. Simple.",
	"version": "0.31",
	"manifest_version": 2,
	"options_page": "options.htm",
	"background": { "page": "background.htm" },
	"content_security_policy": "script-src 'self' https://toolbarqueries.google.com; object-src 'self'",
	"description": "Gets Google's Page Rank and displays it in the address bar",
	"page_action": {
		"default_icon": "images/icon.png",
		"default_title": "Lite Page Rank"
	},
	"icons": 
		{
		"128": "images/icon_128.png" 
		},
	"permissions": [
	    "tabs",
		"http://*/"
	]
}